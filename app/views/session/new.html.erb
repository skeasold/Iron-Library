<%= form_tag do %>
  <div class="">
    <%= label_tag :email %>
    <%= text_field_tag :email %>
  </div>
  <div class="">
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </div>


  <%= submit_tag "Sign In", class: "mdl-button mdl-js-button mdl-button--raised mdl-button--colored" %>

  or
  <%= link_to 'Sign Up', signup_path %>

<% end %>
